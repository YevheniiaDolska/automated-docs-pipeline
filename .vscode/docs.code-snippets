      "## Step 1: Prepare environment",
      "",
      "```bash",
      "# Set production namespace",
      "kubectl create namespace production",
      "kubectl config set-context --current --namespace=production",
      "",
      "# Verify cluster resources",
      "kubectl top nodes",
      "# Ensure each node has >30% free resources",
      "```",
      "",
      "## Step 2: Configure secrets",
      "",
      "```bash",
      "# Create secrets",
      "kubectl create secret generic api-keys \\\\",
      "  --from-literal=database-url='${8:postgresql://...}' \\\\",
      "  --from-literal=api-key='${9:sk_live_...}' \\\\",
      "  --from-literal=jwt-secret='${10:random-256-bit-string}'",
      "",
      "# Verify secrets",
      "kubectl get secrets",
      "```",
      "",
      "## Step 3: Deploy application",
      "",
      "```yaml",
      "# deployment.yaml",
      "apiVersion: apps/v1",
      "kind: Deployment",
      "metadata:",
      "  name: ${1:application}",
      "spec:",
      "  replicas: ${11:3}",
      "  strategy:",
      "    type: RollingUpdate",
      "    rollingUpdate:",
      "      maxSurge: 1",
      "      maxUnavailable: 0",
      "  template:",
      "    spec:",
      "      containers:",
      "      - name: app",
      "        image: ${12:registry.example.com/app:v1.2.3}",
      "        resources:",
      "          requests:",
      "            memory: \"${13:2Gi}\"",
      "            cpu: \"${14:1000m}\"",
      "          limits:",
      "            memory: \"${15:4Gi}\"",
      "            cpu: \"${16:2000m}\"",
      "        livenessProbe:",
      "          httpGet:",
      "            path: /health",
      "            port: ${17:8080}",
      "          initialDelaySeconds: ${18:30}",
      "          periodSeconds: ${19:10}",
      "        readinessProbe:",
      "          httpGet:",
      "            path: /ready",
      "            port: ${17:8080}",
      "          initialDelaySeconds: ${20:5}",
      "          periodSeconds: ${21:5}",
      "```",
      "",
      "```bash",
      "# Deploy",
      "kubectl apply -f deployment.yaml",
      "",
      "# Monitor rollout",
      "kubectl rollout status deployment/${1:application}",
      "# Expected: deployment \"${1:application}\" successfully rolled out",
      "```",
      "",
      "## Step 4: Configure autoscaling",
      "",
      "```yaml",
      "# hpa.yaml",
      "apiVersion: autoscaling/v2",
      "kind: HorizontalPodAutoscaler",
      "metadata:",
      "  name: ${1:application}-hpa",
      "spec:",
      "  scaleTargetRef:",
      "    apiVersion: apps/v1",
      "    kind: Deployment",
      "    name: ${1:application}",
      "  minReplicas: ${11:3}",
      "  maxReplicas: ${22:10}",
      "  metrics:",
      "  - type: Resource",
      "    resource:",
      "      name: cpu",
      "      target:",
      "        type: Utilization",
      "        averageUtilization: ${23:70}",
      "  - type: Resource",
      "    resource:",
      "      name: memory",
      "      target:",
      "        type: Utilization",
      "        averageUtilization: ${24:80}",
      "```",
      "",
      "## Step 5: Verify deployment",
      "",
      "```bash",
      "# Check pods are running",
      "kubectl get pods",
      "# Expected: ${11:3}/${11:3} Running",
      "",
      "# Test endpoint",
      "kubectl port-forward svc/${1:application} 8080:80",
      "curl http://localhost:8080/health",
      "# Expected: {\"status\":\"healthy\"}",
      "",
      "# Check metrics",
      "kubectl top pods",
      "# Expected: CPU <${25:50}%, Memory <${26:70}%",
      "```",
      "",
      "## Performance benchmarks",
